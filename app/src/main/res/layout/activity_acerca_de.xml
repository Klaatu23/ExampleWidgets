<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".AcercaDe">

    <TextView
            android:text="Bienvenido"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/tvt1"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="320dp" android:layout_marginEnd="165dp" android:layout_marginRight="165dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="Salir"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            android:layout_marginTop="192dp"
            app:layout_constraintTop_toBottomOf="@+id/tvt1" android:layout_marginEnd="2dp"
            android:layout_marginRight="2dp" app:layout_constraintEnd_toEndOf="@+id/tvt1"/>
</androidx.constraintlayout.widget.ConstraintLayout>